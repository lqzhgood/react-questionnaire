{
    "name": "react-questionnaire",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@ant-design/icons": "^5.2.6",
        "@craco/craco": "^7.1.0",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.54",
        "@types/react": "^18.2.22",
        "@types/react-dom": "^18.2.7",
        "ahooks": "^3.7.8",
        "antd": "^5.9.4",
        "axios": "^1.5.1",
        "better-mock": "^0.3.6",
        "classnames": "^2.3.2",
        "immer": "^10.0.2",
        "mockjs": "^1.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.16.0",
        "react-scripts": "5.0.1",
        "sass": "^1.69.0",
        "styled-components": "^6.0.8",
        "typescript": "^4.9.5",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "dev": "craco start",
        "build": "craco build",
        "test": "craco test",
        "eject": "craco eject",
        "format": ".\\node_modules\\.bin\\prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest",
            "plugin:prettier/recommended"
        ],
        "overrides": [
            {
                "files": [
                    "**/*.ts?(x)"
                ],
                "rules": {
                    "additional-typescript-only-rule": "warn"
                }
            }
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier --write"
        ]
    },
    "devDependencies": {
        "@types/styled-components": "^5.1.28",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "prettier": "^3.0.3"
    }
}
